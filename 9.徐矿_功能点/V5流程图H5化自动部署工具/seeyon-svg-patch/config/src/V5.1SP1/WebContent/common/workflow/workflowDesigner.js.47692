/**
 * 重新调整flash流程图大小
 * @param width 流程图宽度
 * @param height 流程图高度
 */
function resizeFlash(width, height,isShowButtonInPage,isDialog){
  //alert("width:="+width+"height:="+height);
  var monitorObj = isInternetExplorer ? document.all.monitor : document.monitor;
  if(monitorObj){
      if(!monitorObj.resizeScrollPane){
          setTimeout("resizeFlash("+width+","+height+","+isShowButtonInPage+","+isDialog+")",100);
          return;
      }
      var version = $.browser.version;
      if(width > 0){
          if(isShowButtonInPage){
              if(isDialog){
                if($.browser.msie){//ie
                  width= width-50;
                  height = height - 126;
                }else if($.browser.mozilla){//firefox
                  width= width-50;
                  height = height - 130;
                }else if($.browser.safari){//safari
                  width= width-50;
                  height = height - 130;
                }else if($.browser.chrome){//chrome
                  width= width-50;
                  height = height - 150;
                }else if($.browser.opera){//opera
                  width= width-25;
                  height = height-48;
                }else{
                  width= width-32;
                  height = height - 130;
                }
              }else{
                if($.browser.msie){//ie
                  height = height - 35;
                  if(version=="10.0" || version=="11.0"){
                    height = height - 50;
                    $("#workflowButtonTR").css("height", "80");
                  }
                }else if($.browser.mozilla){//firefox
                  height= height-35;
                }else if($.browser.safari){//safari
                  height= height-35;
                }else if($.browser.chrome){//chrome
                  height= height-35;
                }else if($.browser.opera){//opera
                  height= height-35;
                }else{
                  height= height-35;
                }
                height = height - 30;
              }
          }else{
            if(isDialog){
              if($.browser.msie){//ie
                width= width-53;
                height= height-112;
              }else if($.browser.mozilla){//firefox
                width= width-50;
                height= height-112;
              }else if($.browser.safari){//safari
                width= width-50;
                height= height-112;
              }else if($.browser.chrome){//chrome
                width= width-50;
                height= height-112;
              }else if($.browser.opera){//opera
                width= width;
                height= height;
              }
            }else{
              if($.browser.msie){//ie
                width= width-2;
                height= height-30;
              }else if($.browser.mozilla){//firefox
                //do nothing
                height= height-30;
              }else if($.browser.safari){//safari
                //do nothing
                height= height-30;
              }else if($.browser.chrome){//chrome
                width= width-16;
                height= height-55;
              }else if($.browser.opera){//opera
                width= width-2;
                height= height-30;
              }
            }
          }
          if(scene == "0" || scene == "1"){
        	  height= height-20;
          }
          monitorObj.setAttribute("width", width);
          monitorObj.setAttribute("height", height);
          var result = monitorObj.resizeScrollPane(width, height);
      }
  }else{
      getXML();
  }
}

