		<url-pattern><![CDATA[.*/fileUpload.*]]></url-pattern>
		<url-pattern><![CDATA[.*/PicUploadServlet.*]]></url-pattern>
		<url-pattern><![CDATA[.*/VotePicUploadServlet.*]]></url-pattern>
		<url-pattern><![CDATA[.*/ActivityPicUploadServlet.*]]></url-pattern>
		<url-pattern><![CDATA[.*/*.service.*]]></url-pattern>
		<url-pattern><![CDATA[.*/gVarInit.jsp.*]]></url-pattern>
		<url-pattern><![CDATA[.*/apps.action.*]]></url-pattern>
		<url-pattern><![CDATA[.*/yzmServlet.*]]></url-pattern>
