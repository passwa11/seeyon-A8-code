<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.seeyon.apps.ext.zxzyk.po">
    <class name="CtpOrgUser" table="CTP_ORG_USER_MAPPER" lazy="false">
        <id name="id" type="long" column="ID">
            <generator class="assigned"/>
        </id>
        <property name="type" type="string" column="TYPE"/>
        <property name="loginName" type="string" column="LOGIN_NAME"/>
        <property name="exLoginName" type="string" column="EX_LOGIN_NAME"/>
        <property name="exPassword" type="string" column="EX_PASSWORD"/>
        <property name="exId" type="string" column="EX_ID"/>
        <property name="exUserId" type="string" column="EX_USER_ID"/>
        <property name="memberId" type="long" column="MEMBER_ID"/>
        <property name="exUnitCode" type="string" column="EX_UNIT_CODE"/>
        <property name="description" type="string" column="DESCRIPTION"/>
        <property name="actionTime" type="date" column="ACTION_TIME"/>
    </class>
</hibernate-mapping>
