<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.seeyon.apps.ext.welinkMenu.po">
    <class name="WeLinkUsers" table="WELINK_USERS" lazy="false">
        <id name="oaUserId" type="string" column="OA_USER_ID">
            <generator class="assigned"/>
        </id>
        <property name="weLinkLoginname" type="string" column="WELINK_LOGINNAME"/>
        <property name="weLinkPwd" type="string" column="WELINK_PWD"/>
        <property name="oaUserName" type="string" column="OA_USER_NAME"/>
        <property name="oaPhone" type="string" column="OA_PHONE"/>
    </class>
</hibernate-mapping>
