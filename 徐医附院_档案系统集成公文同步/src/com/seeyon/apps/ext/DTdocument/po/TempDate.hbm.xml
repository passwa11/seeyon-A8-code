<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.seeyon.apps.ext.DTdocument.po">
    <class name="TempDate" table="TEMP_DATE" lazy="false">
        <id name="id" type="long" column="ID">
            <generator class="assigned"/>
        </id>
        <property name="startdate" type="string" column="STARTDATE"/>
        <property name="enddate" type="string" column="ENDDATE"/>
    </class>
</hibernate-mapping>
