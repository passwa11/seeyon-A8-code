<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC 
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
<class name="com.seeyon.apps.govdoc.po.EdocStatSet" lazy="false" table="edoc_stat_set">
	<id
        name="id"
        type="long"
        column="id"
        length="20"
    >
		<generator class="assigned" />
	</id>
	 <property
        name="name"
        type="string"
        column="stat_name"
    />
	 <property
        name="parentId"
        type="long"
        column="parent_id"
        not-null="true"
        length="20"
    />
	<property
        name="accountId"
        type="long"
        column="account_id"
        not-null="true"
        length="20"
    />
    <property
        name="statType"
        type="string"
        column="stat_type"
        length="255"
    />

    <property
        name="recNode"
        type="string"
        column="rec_node"
    />
    
     <property
        name="sendNode"
        type="string"
        column="send_node"
    />
    
    
    <property
        name="modifyTime"
        type="timestamp"
        column="modify_time"
        not-null="true"
        length="19"
    />
    
     <property
        name="deptIds"
        type="string"
        column="dept_ids"
    />
      <property
        name="deptNames"
        type="string"
        column="dept_names"
    />
    <property
    	name="statNodePolicy"
    	type="string"
    	column="stat_node_policy"
    />
    <property
    	name="recNodePolicy"
    	type="string"
    	column="red_node_policy"
    />
    <property
        name="orderNo"
        type="integer"
        column="order_no"
        length="4"
      />
     <property
        name="comments"
        type="string"
        column="comments"
    />
      <property
        name="timeType"
        type="string"
        column="time_type"
    />
       <property
        name="govType"
        type="string"
        column="gov_type"
    />
     <property
        name="initState"
        type="integer"
        column="init_state"
        not-null="true"
        length="4"
    />
   	 <property
        name="state"
        type="integer"
        column="stat_state"
        not-null="true"
        length="4"
    />
</class>
</hibernate-mapping>
