<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC 
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="com.seeyon.apps.trustdo.po">
	<class name="XRDUserPO" table="trustdo_bind_user">
		<id name="id" type="long" column="id" length="20">
			<generator class="assigned" />
		</id>
		<property name="seeyonUserName" type="string" column="seeyon_user_name" not-null="true" length="30" />
		<property name="seeyonUserDept" type="string" column="seeyon_user_dept" not-null="true" length="60" />
		<property name="seeyonLoginName" type="string" column="seeyon_login_name" not-null="true" length="30" />
		<property name="trustdoAccount" type="string" column="trustdo_account" not-null="true" length="30" />
		<property name="bindTime" type="string" column="bind_time" not-null="true" length="30" />
		<property name="keyId" type="string" column="key_id" length="50" />
		<property name="extend2" type="string" column="extend2" length="30" />
		<property name="extend3" type="string" column="extend3" length="30" />
		<property name="extend4" type="string" column="extend4" length="30" />
		<property name="extend5" type="string" column="extend5" length="30" />

	</class>
</hibernate-mapping>
