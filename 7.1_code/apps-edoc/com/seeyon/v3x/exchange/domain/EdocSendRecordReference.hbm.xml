<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC 
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
<class 
    name="com.seeyon.v3x.exchange.domain.EdocSendRecordReference" 
    table="edoc_exchange_send_reference"
    lazy="true"
>
    <id
        name="id"
        type="long"
        column="id"
        length="20"
    >
        <generator class="assigned" />
    </id>
    <property
        name="referenceSendRecodId"
        type="long"
        column="reference_send_record_id"
        length="20"
    /> 
	<property
        name="newSendRecodId"
        type="long"
        column="new_send_record_id"
        length="20"
    /> 
</class>
</hibernate-mapping>