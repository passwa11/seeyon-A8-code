<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC 
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
<class 
    name="com.seeyon.v3x.edoc.domain.EdocCustomerType" 
    table="edoc_customer_type"
    lazy="false"
>
    <id
        name="id"
        type="long"
        column="id"
        length="20"
    >
        <generator class="assigned" />
    </id>
    <property
        name="memberId"
        type="long"
        column="member_id"
    />
    <property
        name="typeId"
        type="long"
        column="type_id"
    />
 	<property
        name="bigTypeId"
        type="long"
        column="big_type_id"
    />
    <property
        name="accountId"
        type="long"
        column="account_id"
    />
    <property
        name="edocType"
        type="long"
        column="edoc_type"
    />
    <property
        name="typeName"
        type="string"
        column="type_name"
    />
    <property
        name="typeCode"
        type="string"
        column="type_code"
    />
</class>
</hibernate-mapping>
