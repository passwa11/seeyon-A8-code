<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC 
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
<class 
    name="com.seeyon.v3x.edoc.domain.EdocStatCondition" 
    table="edoc_stat_condition"
    lazy="false"
>
    <id
        name="id"
        type="long"
        column="id"
        length="20"
    >
        <generator class="assigned" />
    </id>
    
    <property
        name="userId"
        type="long"
        column="user_id"
    />
    <property
        name="accountId"
        type="long"
        column="account_id"
    />
    <property
        name="title"
        type="string"
        column="stat_title"
    />
    <property
        name="createTime"
        type="timestamp"
        column="create_time"
    />
    <property
        name="statisticsDimension"
        type="int"
        column="statistics_dimension"
    />
 	<property
        name="organizationId"
        type="string"
        column="organization_id"
    />
    <property
        name="timeType"
        type="int"
        column="time_type"
    />
    <property
        name="starttime"
        type="string"
        column="start_time"
    />
    <property
        name="endtime"
        type="string"
        column="end_time"
    />
    
    <property
        name="sendContentId"
        type="string"
        column="send_content_id"
    />
    <property
        name="workflowNodeId"
        type="string"
        column="workflow_node_id"
    />
    <property
        name="processSituationId"
        type="string"
        column="process_situation_id"
    />
    <property
        name="sendNodeCode"
        type="string"
        column="send_node_code"
    />
    <property
        name="recNodeCode"
        type="string"
        column="rec_node_code"
    />
    <property
        name="contentExt1"
        type="string"
        column="content_ext1"
    />
    <property
        name="contentExt2"
        type="string"
        column="content_ext2"
    />
    <property
        name="contentExt3"
        type="string"
        column="content_ext3"
    />
    <property
        name="isOld"
        type="java.lang.Boolean"
        column="is_old"
    />
    <property
        name="edocType"
        type="java.lang.Integer"
        column="edoc_type"
    />
    <property
        name="sendType"
        type="java.lang.String"
        column="send_type"
    />
    <property
        name="unitLevel"
        type="java.lang.String"
        column="unit_level"
    />
    <property
        name="operationType"
        type="java.lang.String"
        column="operation_type"
    />
    <property
        name="operationTypeIds"
        type="java.lang.String"
        column="operation_type_ids"
    />
    <property
        name="pushRole"
        type="java.lang.Integer"
        column="push_role"
    />
     <property
        name="pushFrom"
        type="java.lang.Long"
        column="push_from"
    />
</class>
</hibernate-mapping>
