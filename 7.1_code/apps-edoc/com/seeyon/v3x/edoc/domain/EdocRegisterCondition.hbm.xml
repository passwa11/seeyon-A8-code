<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC 
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
<class 
    name="com.seeyon.v3x.edoc.domain.EdocRegisterCondition" 
    table="edoc_register_condition"
    lazy="false"
>
    <id
        name="id"
        type="long"
        column="id"
        length="20"
    >
        <generator class="assigned" />
    </id>
    
    <property
        name="userId"
        type="long"
        column="user_id"
    />
    <property
        name="accountId"
        type="long"
        column="account_id"
    />
    <property
        name="title"
        type="string"
        column="title"
    />
    <property
        name="createTime"
        type="timestamp"
        column="create_time"
    />
    
    <property
        name="starttime"
        type="string"
        column="start_time"
    />
    <property
        name="endtime"
        type="string"
        column="end_time"
    />
    
    <property
        name="type"
        type="int"
        column="type"
    />
    <property
        name="queryCol"
        type="string"
        column="query_col"
    />
    <property
        name="contentExt1"
        type="string"
        column="content_ext1"
    />
    <property
        name="contentExt2"
        type="string"
        column="content_ext2"
    />
    <property
        name="contentExt3"
        type="string"
        column="content_ext3"
    />
    
        
    <property
        name="sendQueryTimeType"
        type="int"
        column="send_query_time_type"
    />
    
</class>
</hibernate-mapping>
