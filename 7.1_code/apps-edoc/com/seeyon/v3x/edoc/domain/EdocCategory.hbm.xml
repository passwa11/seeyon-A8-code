<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
    
<hibernate-mapping>
<class 
	name="com.seeyon.v3x.edoc.domain.EdocCategory"
	table="edoc_category">
	<id 
		name="id"
		type="java.lang.Long"
		column="id"
		length="20"
	/>
	<property
		name="name"
		type="java.lang.String"
		column="name"
		length="255"
	/>
	<property
		name="rootCategory"
		type="java.lang.Long"
		column="root_category"
		length="20"
	/>
	<property
		name="accountId"
		column="account_id"
		type="java.lang.Long"
		length="20"
	/>
	<property
		name="state"
		column="state"
		type="java.lang.Integer"
		length="4"
	/>
	<property
		name="storeType"
		column="store_type"
		type="java.lang.Integer"
		length="2"
	/>
	<property
		name="modifyUserId"
		column="modify_user"
		type="long"
		length="20"
	/>
	<property
		name="modifyTime"
		column="modify_time"
		type="timestamp"
	/>
</class>
</hibernate-mapping>
