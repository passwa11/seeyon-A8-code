CREATE TABLE "EDOC_FORM_INFO" (
  "ID" NUMBER NOT NULL ,
  "FORM_ID" NUMBER ,
  "NEED_NOT_SHOW_DEPT_ID" VARCHAR2(4000) ,
  "NEED_NOT_SHOW_DEPT_NAME" VARCHAR2(4000) ,
  PRIMARY KEY ("ID")
)
;

COMMENT ON COLUMN "EDOC_FORM_INFO"."ID" IS '主键id';

COMMENT ON COLUMN "EDOC_FORM_INFO"."FORM_ID" IS '文单id';

COMMENT ON COLUMN "EDOC_FORM_INFO"."NEED_NOT_SHOW_DEPT_ID" IS '不需要显示的部门ID'
COMMENT ON COLUMN "EDOC_FORM_INFO"."NEED_NOT_SHOW_DEPT_NAME" IS '不需要显示的部门名称'