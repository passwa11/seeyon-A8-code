<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.seeyon.apps.ext.KfqInform.po.KfqInform" table="z_inform_temp">
        <id name="id" type="java.lang.Long">
            <column name="id"/>
            <generator class="assigned"></generator>
        </id>
        <property name="sort" type="int">
            <column name="sort" />
        </property>
        <property name="memberid" type="java.lang.String">
            <column name="memberid" />
        </property>
        <property name="membername" type="java.lang.String">
            <column name="membername"/>
        </property>
        <property name="summaryid" type="java.lang.String">
            <column name="summaryid"/>
        </property>
        <property name="createuserid" type="java.lang.String">
            <column name="createuserid"/>
        </property>

    </class>
</hibernate-mapping>
