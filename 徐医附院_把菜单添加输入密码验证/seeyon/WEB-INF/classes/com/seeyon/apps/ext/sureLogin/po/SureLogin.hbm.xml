<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.seeyon.apps.ext.sureLogin.po">
    <class name="SureLogin" table="Z_SURE_LOGIN" lazy="false">
        <id name="id" type="long" column="ID">
            <generator class="assigned"/>
        </id>
        <property name="loginName" type="string" column="LOGINNAME"/>
        <property name="password" type="string" column="PASSWORD"/>
        <property name="answer1" type="string" column="ANSWER1"/>
        <property name="answer2" type="string" column="ANSWER2"/>
        <property name="answer3" type="string" column="ANSWER3"/>
    </class>
</hibernate-mapping>
