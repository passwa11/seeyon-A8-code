<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.seeyon.apps.xkjt.po">
	<class name="XkjtOpenMode" table="open_mode" lazy="false">
		<id name="id" type="long" column="ID">
			<generator class="assigned" />
		</id>
		<property name="nodeId" type="long" column="NODE_ID" />
		<property name="roleIds" type="string" column="ROLE_IDS" />
		<property name="isDeleted" type="integer" column="IS_DELETED" />
	</class>
</hibernate-mapping>